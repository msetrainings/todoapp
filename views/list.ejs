<%- include("header") -%>
<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>
<div class="box">
    <% newListItems.forEach((item) => { %>
        <div class="item">
            <input type="checkbox">
            <p> <%= item %></p>
        </div>
        <% }) %>
    
    <form action="/" method="post" class="item">
        <input type="text" name="todoTask" placeholder="Add in todo list" autocomplete="off" required>
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
</div>
<%- include("footer") -%>